          <text>优惠活动</text>
          <navigator wx:if="{{jigou_info.shenqing_user_id == user_id }}" url='/pages/ruzhu/fabu' class='fabu_yh fl_r'>发布优惠</navigator>
        </view>  
        <navigator url='/pages/listdetail/listdetail?brand_id={{jigou_info.brand_id}}' class='flex_h'>
          <text class='hui'>更多</text>
          <image class='right' src='/images/x_right.png'></image>
        </navigator>
      </view>
      <view></view>
      <block wx:for="{{xiangmu_list}}" wx:key="key">
      <navigator  class="pr" url="../product/detail?productId={{item.goods_id}}" hover-class="changestyle">
        <image wx:if="{{item.is_huodong == 1}}" class="jiaobiao" src="/images/huodong.png"></image>
        <image lazy-load class="photo" mode='aspectFill' src="http://www.juziyimei.com/{{item.goods_img}}"></image>
        <view class='h40 flex_h'>
          <view class="title">{{item.goods_name}}</view>
        </view>
        <view class='flex_h font_14 l_h20 mtl-5'>
          <view class="juzi_cor">优惠价{{item.juzi_price}}元</view>
        </view>
        <view class='goods_bottom' >
          <view class="gmx">原价{{item.market_price}}元</view>
        </view>
       <view class='font_11 no_next_text hui'>
        <text wx:if="{{user_lei == 99 && item.is_huodong == 1}}" >返佣：￥{{item.huodong_3}}/{{item.huodong_2}}/{{item.huodong_1}}</text> 
          <text wx:if="{{user_lei == 3 && item.is_huodong == 1}}" >返佣：￥{{item.huodong_1}}</text> 
          <text wx:if="{{user_lei == 2 && item.is_huodong == 1}}">返佣：￥{{item.huodong_2}}</text>  
          <text wx:if="{{user_lei == 1 && item.is_huodong == 1}}">返佣：￥{{item.huodong_3}}</text> 
          </view>
      <view wx:if="{{jigou_info.shenqing_user_id == user_id }}" class='flex_h_b'>
          <view class='font_11 no_next_text hui' wx:if="{{item.off == 1}}">不公开显示</view>
          <navigator url='/pages/ruzhu/fabu?goods_id={{item.goods_id}}' class='fabu_yh fl_r mt5 mb5'>编辑活动</navigator>
      </view>
      </navigator>
      </block>
  <navigator url='/pages/listdetail/listdetail?brand_id={{jigou_info.brand_id}}'  class='bte mt15 no_more l_h50 ff6633'>查看全部优惠项目</navigator>
  <!--优惠活动-->
</view>
  <!--医师-->
<view wx:if="{{ys == 0}}" class='jianjie' id="yishi">
  <view class='font_14 flex_h_b mb10 ml10 font_weight l_h40' ><text>专业团队</text></view>
  <!--推荐好物内容-->
          <swiper wx:if="{{ys_list.length > 0}}" display-multiple-items="{{count_ys}}" indicator-dots="{{indicatorDots}}" previous-margin="0px" next-margin="0px" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" style='height:200px;'>
          
            <block >
              <swiper-item wx:for="{{ys_list}}"  wx:key="key"  wx:if="{{item.lx == 2}}">
                <navigator url="../jigou/jigou?jigou_id={{item.brand_id}}&ys=1" class='tjhw flex_s f4f8f8 m5'>
                  <image class='pl_img' mode='aspectFill' src='{{item.brand_logo}}'></image>
                  <text class='font_16'>{{item.brand_name}}</text>
                  <text class='font_12 hui'>{{item.jibie}}</text>
                  <text class='font_14'>详细介绍></text>
                </navigator>
              </swiper-item>
              <swiper-item wx:for="{{ys_list}}"  wx:key="key"  wx:if="{{item.lx == 3}}">
                <navigator url="../jigou/jigou?jigou_id={{item.brand_id}}&ys=1" class='tjhw flex_s f4f8f8 m5'>
                  <image class='pl_img' mode='aspectFill' src='{{item.brand_logo}}'></image>
                  <text class='font_16'>{{item.brand_name}}</text>
                  <text class='font_12 hui'>美学顾问</text>
                  <text class='font_14'>详细介绍></text>
                </navigator>
              </swiper-item>
            </block>
          </swiper>
          <view wx:else>
               <view  class='l_h90 no_more'>暂无</view>
            </view>
