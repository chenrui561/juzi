        <text class='jigou_title ovh1'>{{jigou_info.brand_name}}</text>
        <view class='flex_h_b font_12 hui l_h30'>
          <text class='mr5'>效果{{jigou_fen.xiaoguo}}</text>
          <text class='mr5'>服务{{jigou_fen.fuwu}}</text>
          <text class='mr5'>成交率{{jigou_fen.cjl}}</text>
