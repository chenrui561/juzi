           <image wx:if="{{jigou_fen.fen == 6}}" mode='widthFix' class='pjxx' src='https://www.juziyimei.com/images/star03.png'></image>
           <image wx:if="{{jigou_fen.fen == 5}}" mode='widthFix' class='pjxx' src='https://www.juziyimei.com/images/star2.5.png'></image>
           <image wx:if="{{jigou_fen.fen == 4}}" mode='widthFix' class='pjxx' src='https://www.juziyimei.com/images/star02.png'></image>
           <image wx:if="{{jigou_fen.fen == 3}}" mode='widthFix' class='pjxx' src='https://www.juziyimei.com/images/star1.5.png'></image>
           <image wx:if="{{jigou_fen.fen == 2}}" mode='widthFix' class='pjxx' src='https://www.juziyimei.com/images/star01.png'></image>
           <view wx:if="{{ys == 0}}" class='ml5 hui flex_h_b font_12'>
             <text>更多</text>
             <image mode='widthFix' class='phone_img2' src='/images/x_right.png'></image>
          </view>
        </view>
      </view> 
    </navigator>
    <!--医院标题第一部分end-->   
    <!--医院标题第二部分star-->
    <view class='jigou_info2'>
      <view class='flex_h_b'>
        <view class='flex_s w30' data-current="4"  bindtap="indexChange">
          <text class='font_18 c3 '>{{count.pj}}</text>
          <text class='font_14 hui'>评论</text>
        </view>
        <navigator url='/pages/jigou/jigou_info?currentTab=3&brand_id={{jigou_info.brand_id}}' class='flex_s w30'>
          <text class='font_18 c3 '>{{count.anli}}</text>
          <text class='font_14 hui'>案例</text>
        </navigator>
        <view wx:if="{{ys == 0}}" class='flex_s w30' data-current="3"  bindtap="indexChange">
          <text class='font_18 c3 '>{{count.yisheng}}</text>
          <text class='font_14 hui'>团队</text>
        </view>
        <navigator url='/pages/jigou/jigou_info?currentTab=1&brand_id={{jigou_info.brand_id}}' wx:if="{{ys == 1}}" class='flex_s w30'>
          <text class='font_18 c3 '>{{count.zizhi}}</text>
          <text class='font_14 hui'>资质</text>
        </navigator>
      </view>
  
      <view wx:if="{{ys == 0}}" class='flex_h_b mt15'>
        <view class='flex_s w30' data-current="2" bindtap="indexChange">
          <image mode='widthFix' class='phone_img' src='/images/youhui.png'></image>
          <text class='font_14 '>优惠项目</text>
        </view>
        <navigator wx:if="{{ys == 0}}" url='/pages/jigou/jigou_info?currentTab=1&brand_id={{jigou_info.brand_id}}' class='flex_s w30'>
          <image mode='widthFix' class='phone_img' src='/images/zige.png'></image>
          <text wx:if="{{jigou_info.lx == 0}}" class='font_14 '>执业资格</text>
          <text wx:if="{{jigou_info.lx == 1}}" class='font_14 '>资质证件</text>
        </navigator>
        <navigator wx:if="{{ys == 0}}"  url='/pages/jigou/jigou_info?currentTab=2&brand_id={{jigou_info.brand_id}}' class='flex_s w30'>
          <image mode='widthFix' class='phone_img' src='/images/hj.png'></image>
          <text wx:if="{{jigou_info.lx == 0}}" class='font_14 '>机构环境</text>
          <text wx:if="{{jigou_info.lx == 1}}" class='font_14 '>店面环境</text>
        </navigator>
      </view>
    </view>



  </view>
    <!--擅长项目-->
      <view wx:if="{{ys == 1}}" class='flex ml20 ' style="width:90%;margin-left:5%;margin-top:10px;">
        <view style="white-space:nowrap;" class='font_14 font_weight l_h32'>擅长项目:</view>
        <view class='flex_h wrap'><view wx:for="{{scxm}}" wx:key="key" class='font_12 bg_hui scxm l_h24'>{{item}}</view></view>
  
      </view>
