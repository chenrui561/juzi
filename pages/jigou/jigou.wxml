      </view>
      <!--处理用户头像结束-->
      <!--用户名和用户留言-->
      <view class="comment_sd font_14">
        <view class='flex_s_l '>
          <text class='l_h20' >{{item.passwd_answer}}</text>
          <text class=' font_14 hui l_h20'>{{item.add_time}}</text>
        </view>
        <view class='pingfen'>
          <view class='flex_h_b l_h24'>
              <text class='font_14 hui mr5'>机构评价:</text>
              <image lazy-load mode='widthFix' class='pjxx' src='/images/star{{item.fen}}.png'></image>
            </view>
            <view  class='flex_h_b l_h24' wx:if="{{item.yisheng_id > 0}}">
              <text class='font_14 hui mr5'>{{item.ys_name}}评价:</text>
              <image lazy-load mode='widthFix' class='pjxx' src='/images/star{{item.fen2}}.png'></image>
            </view>
            <view  class='flex_h_b l_h24' wx:if="{{item.zixun_id > 0}}">
