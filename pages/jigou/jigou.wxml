           <image wx:if="{{jigou_fen.fen == 9}}" mode='widthFix' class='pjxx' src='https://www.juziyimei.com/images/star4.5.png'></image>
           <image wx:if="{{jigou_fen.fen == 8}}" mode='widthFix' class='pjxx' src='https://www.juziyimei.com/images/star04.png'></image>
