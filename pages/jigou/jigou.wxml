              <image lazy-load mode='widthFix' class='pjxx' src='/images/star{{item.fen3}}.png'></image>
            </view>
          </view>
      </view>
      <view class="comment_sd mt15 font_14">
        <text>{{item.content}}</text>
      </view>
      
      <!--用户名和用户留言结束-->
      <!--如果是一张图的情况-->
      <view wx:if="{{item.comment_img && !item.comment_img2}}" class="comment_sd onbig_img" style="display: -webkit-box; ">
        <image lazy-load bindtap="big_img" data-bigimg="{{item.img_arr}}" data-img="{{item.comment_img}}" mode='aspectFit' src="{{item.comment_img}}" style="max-width: 250px;max-height: 180px;"></image>
      </view>
      <!--如果是大于一张图的情况-->
      <view wx:if="{{item.comment_img2}}">
        <view  class="comment_sd onbig_img" style="width:calc(100% - 40px);">
          <image lazy-load mode='aspectFill' bindtap="big_img" data-bigimg="{{item.img_arr}}" data-img="{{item.comment_img}}" wx:if="{{item.comment_img}}" class='pl_img' src='{{item.comment_img}}'></image>
          <image lazy-load mode='aspectFill' bindtap="big_img" data-bigimg="{{item.img_arr}}" data-img="{{item.comment_img2}}" wx:if="{{item.comment_img2}}" class='pl_img' src='{{item.comment_img2}}'></image>
          <image lazy-load mode='aspectFill' bindtap="big_img" data-bigimg="{{item.img_arr}}" data-img="{{item.comment_img3}}" wx:if="{{item.comment_img3}}" class='pl_img' src='{{item.comment_img3}}'></image>
        </view>
        <view class="comment_sd onbig_img" style="width:calc(100% - 40px);">
          <image lazy-load mode='aspectFill' bindtap="big_img" data-bigimg="{{item.img_arr}}" data-img="{{item.comment_img4}}" wx:if="{{item.comment_img4}}" class='pl_img' src='{{item.comment_img4}}'></image>
          <image lazy-load mode='aspectFill' bindtap="big_img" data-bigimg="{{item.img_arr}}" data-img="{{item.comment_img5}}" wx:if="{{item.comment_img5}}" class='pl_img' src='{{item.comment_img5}}'></image>
          <image lazy-load mode='aspectFill' bindtap="big_img" data-bigimg="{{item.img_arr}}" data-img="{{item.comment_img6}}" wx:if="{{item.comment_img6}}" class='pl_img' src='{{item.comment_img6}}'></image>
        </view>
        <view class="comment_sd onbig_img" style="width:calc(100% - 40px);">
          <image lazy-load mode='aspectFill' bindtap="big_img" data-bigimg="{{item.img_arr}}" data-img="{{item.comment_img7}}" wx:if="{{item.comment_img7}}" class='pl_img' src='{{item.comment_img7}}'></image>
          <image lazy-load mode='aspectFill' bindtap="big_img" data-bigimg="{{item.img_arr}}" data-img="{{item.comment_img8}}" wx:if="{{item.comment_img8}}" class='pl_img' src='{{item.comment_img8}}'></image>
          <image lazy-load mode='aspectFill' bindtap="big_img" data-bigimg="{{item.img_arr}}" data-img="{{item.comment_img9}}" wx:if="{{item.comment_img9}}" class='pl_img' src='{{item.comment_img9}}'></image>
        </view>
      </view>
      <view>
      </view>


    </view>
  </view>

<!--评论列表结束-->
  <view class='l_h60 no_more'>暂无更多评论</view>
</view>
