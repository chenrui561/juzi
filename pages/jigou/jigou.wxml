<!--申请中-->
<view wx:if="{{jigou_info.off == 2}}" class='shenhe_ing'>申请审核中...</view>
<view wx:if="{{jigou_info.off == 0}}" class='shenhe_ing'>申请未提交...</view>
<view wx:if="{{jigou_info.off == 3}}" class='shenhe_ing'>申请已拒绝...</view>
<view wx:if="{{top == 1}}" class='index_head'>
    <scroll-view  scroll-x class="swiper-tab flex_h_b" >
      <view class="swiper-tab-list2 {{z_index==1 ? 'swiper-tab-color' : ''}}" data-current="1" bindtap="indexChange">主页</view>
      <view class="swiper-tab-list2 {{z_index==2 ? 'swiper-tab-color' : ''}}" data-current="2" bindtap="indexChange">优惠</view>
      <view wx:if="{{ys == 0}}" class="swiper-tab-list2 {{z_index==3 ? 'swiper-tab-color' : ''}}" data-current="3" bindtap="indexChange">团队</view>  
      <view class="swiper-tab-list2 {{z_index==4 ? 'swiper-tab-color' : ''}}" data-current="4"  bindtap="indexChange">评价</view>  
    </scroll-view>
</view>
<!--已通过-->
<scroll-view class='scroll-view' scroll-with-animation="true" scroll-y="true" scroll-top="{{s_top}}" bindscroll="bindscroll">
<view class='bj' ></view>
<view class='bj_top' ></view>
<view class='bj_c '>

  <view class='jigou_info flex_s ' >
  <!--医院标题第一部分-->
    <navigator url='/pages/jigou/jigou_info?currentTab=0&brand_id={{jigou_info.brand_id}}' wx:if="{{bianji == 0}}" class='jigou_info1'>
