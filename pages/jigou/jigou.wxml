  <view wx:if="{{bianji == 0}}" class='jigou_lb pr' style='color:#f20'>
    <view wx:if="{{jigou_info.brand_desc == ''}}" class='lb_text white '>{{ys == 1 ?'该医生暂无简介...':'该店铺暂无简介...'}}</view>
    <view wx:else bindtap='text_long' data-id='{{text_long}}' class="lb_text white {{text_long == 1?'text_long':''}} ">{{jigou_info.brand_desc}}</view>
  </view>
  <view wx:if="{{bianji == 1}}" class='jigou_lb pr' style='color:#f20'>
    <textarea bindinput="jianjie_input" placeholder="{{ys == 1 ?'请输入医生简介':'请输入店铺简介'}}" placeholder-class="white" auto-height="true" value='{{jigou_info.brand_desc}}' class='lb_text ovh3 white'></textarea>
  </view>
  -->
</view>

  <!--店铺地址-->
  <block wx:if="{{ys == 0}}">
  <view wx:if="{{bianji == 0}}" bindtap="ditu"  class='jigou_address'>
    <view class='addimg'>
      <icon class="iconfont icon-dizhi jigou_color"></icon> 
      <view class='add_text'>{{jigou_info.address}}</view>
    </view>
    <image class='right' src='/images/x_right.png'></image>
  </view>
  </block>

<view wx:if="{{ys == 0}}" class='cont pr' id="youhui">
<!--优惠活动-->
      
