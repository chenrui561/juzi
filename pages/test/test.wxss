/* pages/test/test.wxss */
.name{
  margin-left: 10px;
  margin-bottom: 15px;
  margin-top: 15px;
  width: 100%;
  display: inline-block;
}
.xm_title{
  padding-left:20px;
  border-left:3px solid #F14918;
  font-size: 14px;
}
.item{
  background-color: #fff;
  margin-top: 10px;
}
.menu2{
  display: inline-block;
    background-color: #f5f5f5;
    box-sizing: border-box;
    border-radius: 3px;
    margin-right: 15px;
    text-align: center;
    height: 32px;
    line-height: 32px;
    padding: 0 8px;
    margin-top: 5px;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.menu2-list{
  padding-bottom: 20px;
  margin-left: 10px;
}
.son-name{
  display: inline-block;
    width: 100%;
    height: 32px;
    line-height: 32px;
    font-size: 13px;
    color: #333;
    letter-spacing: 0;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}