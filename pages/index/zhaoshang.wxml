    <view class="cont">
      <navigator wx:for="{{xzzzp}}" class="pr" url="../product/detail?productId={{item.goods_id}}" hover-class="changestyle">
        <form bindsubmit='formid' report-submit="true">
        <button formType="submit" class='no_button'>
        <image wx:if="{{item.is_huodong == 1}}" class="jiaobiao" src="/images/huodong.png"></image>
        <image class="photo" mode='aspectFill' src="http://www.juziyimei.com/{{item.goods_img}}"></image>
        <view class='h40 flex_h'>
          <view class="title">{{item.goods_name}}</view>
        </view>

        <view class='flex_h font_14 l_h20 mtl-5'>
          <view class="juzi_cor">优惠价：{{item.juzi_price}}元</view><view class="gmx">{{item.market_price}}元</view>
        </view>
        <view class="hui font_12 l_h20 mtl-5"><text class='juzi_cor'>{{item.shop_price}}</text>元预约优惠价</view>
        <view class='goods_bottom' >
          <view class="juli">{{item.yy_num}}人预约</view>
          <view class='flex_h_b brand_name'>
            <image style="width:12px;height:12px; float: left;flex-shrink:0;" src="/images/shop.png"></image>
            <view style="height: 20px;line-height: 20px;text-align: center;margin-left:2px;">{{item.brand_name}}</view>
          </view>
        </view>
            <text class="juli" wx:if="{{user_lei == 99}}">返佣：{{item.huodong_1}}/{{item.huodong_2}}/{{item.huodong_3}}</text>
            <text class="juli" wx:if="{{user_lei == 1}}">返佣：{{item.huodong_1}}</text>
            <text class="juli" wx:if="{{user_lei == 2}}">返佣：{{item.huodong_1}}/{{item.huodong_2}}</text>
            <text class="juli" wx:if="{{user_lei == 3}}">返佣：{{item.huodong_3}}/{{item.huodong_2}}/{{item.huodong_3}}</text>
        </button>
        </form>
      </navigator>
    </view>

