        <view class='flex_s w50'>
          <navigator url='../index/fenxiang?qd_id={{qd_id}}' class="fenxiang_tp">
            <image class="icon_wx_pyq"src="/images/pyq.png"></image>
          </navigator>
          <view class='font_14 haoyou'>图片</view>
        </view>
      </view>
  </view>
