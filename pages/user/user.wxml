<navigator wx:if="{{u_info.user_lei == 0}}" url="../user/team-user?team_type=1" class="qbdd" style='border-bottom-left-radius: 10rpx;border-bottom-right-radius: 10rpx;border-top-left-radius:0px;border-top-right-radius:0px;'>
