<!--pages/user/user.wxml-->
<view class="page">
    <view url="../user/bd" class="userinfo">
      <view class='user_qr'>
          <view bindtap='qr' style='background-image:url(https://www.juziyimei.com/images/qr.png);' class='qr_img'><image src='{{qr_img}}' style='display:none;'></image></view>
          <navigator url="../user/bd" wx:if="{{u_info.mobile_phone == '' && u_info.user_lei == 0}}" class='huiyuan' >游客  ></navigator>
          <navigator url="" wx:if="{{u_info.user_lei == 0 && u_info.mobile_phone !== ''}}" class='huiyuan' >用户  ></navigator>
          <navigator url="../user/team" wx:if="{{u_info.user_lei == 1}}" class='huiyuan' >总监  ></navigator>
          <navigator url="../user/team" wx:if="{{u_info.user_lei == 2}}" class='huiyuan' >城市合伙人  ></navigator>
          <navigator url="../user/team" wx:if="{{u_info.user_lei == 3}}" class='huiyuan' >省联创  ></navigator>
          <navigator url="../user/team" wx:if="{{u_info.user_lei == 9}}" class='huiyuan' >医院  ></navigator>
          <navigator url="../user/team" wx:if="{{u_info.user_lei == 10}}" class='huiyuan' >内部员工  ></navigator>
          <navigator url="../user/team" wx:if="{{u_info.user_lei == 99}}" class='huiyuan' >管理员  ></navigator>
      </view>    
          <navigator wx:if="{{mobile_phone}}" url='../user/bd' class='user_info'>
            <image class="userinfo-avatar fl_l" src="{{u_info.tx_img}}" background-size="cover"></image>
            <view class='user_text'>
              <text class="fl_l" style="font-size: 16px;line-height:20px;" >{{u_info.nick_name}}</text>
              <text  class="fl_l" style="font-size: 14px;line-height:20px;" >{{mobile_phone}}</text>
            </view>
          </navigator>
          <navigator wx:else url='../user/bd' class='user_info'>
            <image class="userinfo-avatar fl_l" src="{{u_info.tx_img}}" background-size="cover"></image>
            <view class='user_text'>
              <text class="fl_l" style="font-size: 16px;line-height:20px;" >{{u_info.nick_name}}</text>
           
              <text class="fl_l" style="font-size: 14px;line-height:20px;" >您还未绑定手机号</text>
            </view>
          </navigator>
      </view>
    <view class="sc1 df">
        <view class="df_1 tc">
          <navigator url="../user/quan" hover-class="none">
            <view class='cash'>{{b_count}}</view>
            <text>优惠券</text>
          </navigator>
        </view>
        <view class="df_1 tc">
          <navigator url="../user/team-shouyi" hover-class="none">
            <view class='cash'><text style="font-size:11px;">￥</text>{{all_money}}</view>
            <text>钱包余额</text>
          </navigator>
        </view>
    </view> 
    <!--订单详情-->
    <view class="qbdd" style='border-bottom: 1px solid #f2f2f2'>
    <span class="font_14 c3">我的订单</span>
    <navigator class="fl_r c9 font_12 l_h20" url="../user/dingdan?currentTab=0" hover-class="none" >全部订单 <span class="fl_r c9" hover-class="none"> <image style='width: 11px;height: 10px;margin-left: 5px;' src="/images/x_right.png"></image></span></navigator>
    </view>
    <view class="sc df tc " style='margin:0px 10px'>
    <navigator class="df_1" url="../user/dingdan?currentTab=0&otype=pay" hover-class="none" >
        <view > 
        <image class='dd_box' src="/images/dfk.png"><view wx:if="{{count1 > 0}}" class='car_count juzi_cor'>{{count1}}</view></image> 
       </view>
        <text class="font_12">待付款<text style="color:red"></text></text>
      </navigator>

     <navigator class="df_1" url="../user/dingdan?currentTab=1&otype=deliver" hover-class="none" >
      <view><image class='dd_box' src="/images/dfh.png"><view wx:if="{{count2 > 0}}" class='car_count juzi_cor'>{{count2}}</view></image></view>
        <text class="font_12">已预约</text>      
      </navigator> 

      <navigator class="df_1" url="../user/dingdan?currentTab=2&otype=receive" hover-class="none" >
          <view><image class='dd_box' src="/images/dpl.png"><view wx:if="{{count3 > 0}}" class='car_count juzi_cor'>{{count3}}</view></image></view>
          <text class="font_12">已完成<text style="color:red"></text></text>
      </navigator>

    <navigator class="df_1" url="../user/dingdan?currentTab=3&otype=finish" hover-class="none" >
          <view><image class='dd_box' src="/images/tksh.png"><view wx:if="{{count4 > 0}}" class='car_count juzi_cor'>{{count4}}</view></image></view>
          <text  class="font_12">已取消<text style="color:red"></text></text>
      </navigator>

    
    <!--<navigator class="df_1" url="../user/dingdan?currentTab=4&otype=fenxiang" hover-class="none" >
          <view><image src="/images/tksh.png"></image></view>
          <text class="font_12">拼团订单<text style="color:red"></text></text>
       </navigator>-->
    </view>



<navigator url="../cart/cart" class="qbdd" style='margin:10px 10px 0px 10px;border-bottom-left-radius: 10rpx;border-bottom-right-radius: 10rpx;'>
 <view >
    <span class="font_14 c3">购物车</span>
    <span class="fl_r c9" hover-class="none"> {{count_car}}<image style='width: 11px;height: 10px;margin-left: 5px;' src="/images/x_right.png"></image></span>
    </view>
</navigator>


<navigator url="/pages/user/shoucang" class="qbdd" style='margin:10px 10px 0px 10px'>
 <view >
    <span class="font_14 c3">我的收藏</span>
    <span class="fl_r c9" hover-class="none"> <image style='width: 11px;height: 10px;margin-left: 5px;' src="/images/x_right.png"></image></span>
