<view class='page'>
<!--术后三张图/术前三张图-->
  <view>
    <swiper style=' height:100VW' indicator-dots="true">
      <swiper-item wx:if="{{anli_info.comment_img}}">
        <image class='jiaobiao' src='/images/after.png'></image>
        <image mode='aspectFill' src="{{anli_info.comment_img}}" class="slide-image"/>
      </swiper-item>
      <swiper-item wx:if="{{anli_info.comment_img2}}">
        <image class='jiaobiao' src='/images/after.png'></image>
        <image mode='aspectFill' src="{{anli_info.comment_img2}}" class="slide-image"/>
      </swiper-item>
      <swiper-item wx:if="{{anli_info.comment_img3}}">
        <image class='jiaobiao' src='/images/after.png'></image>
        <image mode='aspectFill' src="{{anli_info.comment_img3}}" class="slide-image"/>
      </swiper-item>
      
      <swiper-item wx:if="{{anli_info.comment_img4}}">
        <image class='jiaobiao' src='/images/bf.png'></image>
        <image mode='aspectFill' src="{{anli_info.comment_img4}}" class="slide-image"/>
      </swiper-item>
      <swiper-item wx:if="{{anli_info.comment_img5}}">
        <image class='jiaobiao' src='/images/bf.png'></image>
        <image mode='aspectFill' src="{{anli_info.comment_img5}}" class="slide-image"/>
      </swiper-item>
      <swiper-item wx:if="{{anli_info.comment_img6}}">
        <image class='jiaobiao' src='/images/bf.png'></image>
         <image mode='aspectFill' src="{{anli_info.comment_img6}}" class="slide-image" />
      </swiper-item>
     
    </swiper>
  </view>
<!--用户基本信息和分享的内容-->
<view class='user_con'>
  <view class='user_info'>
    <view class='user_info2'>
      <image src='{{anli_info.tx_img}}' class='user_img' mode='aspectFill'></image>
      <text class='user_name'>{{anli_info.passwd_answer}}</text>
      <view wx:if="{{guanzhu_off == 0}}" bindtap='guanzhu' class='guanzhu'>关注</view>
      <view wx:if="{{guanzhu_off > 0}}" bindtap='del_guanzhu' class='guanzhu2'>已关注</view>
    </view>
    <view class='user_time'>{{anli_info.add_time}}</view>
  </view>
  <import src="../../wxParse/wxParse.wxml"/> 
    <view class="wxParse user_connet">
        <view  >
           <template is="wxParse" data="{{wxParseData:content.nodes}}"/>
        </view>
    </view>

</view>
